{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Build Project",
      "program": "${workspaceFolder}\\src\\app.ts",
      "preLaunchTask": "npm: build",
      "sourceMaps": true,
      "smartStep": true,
      "internalConsoleOptions": "openOnSessionStart",
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ]
    }
  ],
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "jest": "^29.2.2",
    "jest-cli": "^29.2.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
